# . startup .
exec dbus-update-activation-environment --all
exec foot -s
exec sh $XDG_CONFIG_HOME/sway/scripts/autostart.sh


# . bar .
bar {
    swaybar_command waybar
}


# . modules .
include {
    "modules/variables"
